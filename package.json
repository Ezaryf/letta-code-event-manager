{
  "name": "letta",
  "version": "3.0.0",
  "type": "module",
  "description": "AI-Powered Coding Assistant - Real-time code analysis, auto-fixes, and intelligent commit generation",
  "main": "scripts/cli.js",
  "bin": {
    "letta": "./scripts/cli.js"
  },
  "scripts": {
    "start": "node scripts/cli.js",
    "watch": "node scripts/assistant.js",
    "fix": "node scripts/autoTestFix.js",
    "chat": "node scripts/cli.js chat",
    "setup": "node scripts/createAgent.js",
    "setup:force": "node scripts/createAgent.js --force",
    "setup:upgrade": "node scripts/createAgent.js --upgrade",
    "cleanup": "node scripts/cleanupAgents.js",
    "help": "node scripts/cli.js --help",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:property": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=property"
  },
  "keywords": ["ai", "coding-assistant", "code-analysis", "letta", "cli"],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@letta-ai/letta-client": "^1.6.2",
    "chalk": "^5.6.2",
    "chokidar": "^5.0.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.3",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "fast-check": "^3.22.0",
    "jest": "^29.7.0"
  }
}
