{
  "name": "letta",
  "version": "3.1.0",
  "type": "module",
  "description": "AI-Powered Developer Cognitive Engine - Real-time code analysis, auto-fixes, intelligent debugging, and flow protection",
  "main": "scripts/cli.js",
  "bin": {
    "letta": "./scripts/cli.js"
  },
  "scripts": {
    "start": "node scripts/cli.js",
    "watch": "node scripts/assistant.js",
    "demo": "node scripts/dashboardDemo.js",
    "demo:forest": "node scripts/dashboardDemo.js forest",
    "demo:sunset": "node scripts/dashboardDemo.js sunset",
    "demo:midnight": "node scripts/dashboardDemo.js midnight",
    "fix": "node scripts/autoTestFix.js",
    "chat": "node scripts/cli.js chat",
    "setup": "node scripts/createAgent.js",
    "setup:force": "node scripts/createAgent.js --force",
    "setup:upgrade": "node scripts/createAgent.js --upgrade",
    "cleanup": "node scripts/cleanupAgents.js",
    "help": "node scripts/cli.js --help",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:property": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=property"
  },
  "keywords": [
    "ai",
    "coding-assistant",
    "code-analysis",
    "letta",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@letta-ai/letta-client": "^1.6.2",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "chokidar": "^5.0.0",
    "cli-table3": "^0.6.5",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "figures": "^6.1.0",
    "fs-extra": "^11.3.3",
    "gradient-string": "^3.0.0",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "fast-check": "^3.22.0",
    "jest": "^29.7.0"
  }
}
